<Window x:Class="Ejercicio5.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Ejercicio5"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel VerticalAlignment="Center" Margin="150">
            <TextBox Name="tbEntrada" SelectionChanged="tbEntrada_SelectionChanged"/>
            
            <TextBox Margin="0,20,0,0" Name="tbSalida" IsReadOnly="True"/>
        </StackPanel>
    </Grid>
</Window>
